<%- include("../partials/header") %>
<% if(currentUser.username === "krates"){ %>
<div class="col-sm-9 row">
    <% } else { %>
        <div class="row">
        <% } %>
    <% for(var i=0;i<offurl.length;i++){ %>
    <% if(counte[0].count % offurl[i].priority === 0){%>
    <div class="col-sm-4">
        <div class="card text-white bg-secondary mb-3 topclass">
            <div class="card-header">OFFER <%= i+1 %></div>
            <div class="card-body">
            <a href="<%= offurl[i].offerurl %>" class="open" target="_blank"><button class="btn btn-success wid">Link</button></a>
            </div>
        </div>
    </div>
    <% }} %>
</div>
<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
