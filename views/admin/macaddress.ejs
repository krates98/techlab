<%- include("../partials/header") %>
<%- include("../partials/adminnav") %>
  
<div class="col-sm-11">
    <% for(var i=0;i<macadd.length;i++){ %>
        <div class="backgroundoffer form-group row">
            <label class="col-sm-3 col-form-label"><strong>MAC #<%= i+1 %></strong></label>
            <form action="/admin/macaddress/<%=macadd[i]._id%>/?_method=DELETE" method="POST">
            <div class="col-sm-9">
                <div class="input-group input2">                
                <input class="form-control formoffers" type="text" value="<%= macadd[i].macaddress %>" readonly>
                <div class="input-group-append">
                    <button class="btn btn-danger">Delete</button>
                  </div>
                </div>
            </div>
            </form>
        </div>
    <% } %>
    <hr>
  
    <form action="/admin/macaddress" method="POST">
        <fieldset>
        
        <!-- Form Name -->
        <h4>Add Mac </h4>
        
        <!-- Text input-->
        <div id="itemsfor" class="form-group">
          
            <div class="margin-bottom">
            <input id="textinput" name="macaddress" type="text" placeholder="Enter MAC Address of Machine" class="form-control input-md ">
            
            </div>
        </div>
        
        <button class="btn btn-info">Submit</button>
        <hr>
    </form>
    </fieldset>
