            <%  if(noMatch === "has"){ %>
                <table class="table">
                    <tr>
                        <th>Count</th>
                        <th>Date</th>
                        <th>Username</th>
                        <th>IP</th>
                        <th>Time</th>
                    </tr>
            <% ipad.forEach(function(campground,index){ %>
                <tr>
                    <td><%= index+1 %></td>
                    <td><%= campground.date %></td>
                    <td><%= campground.username %></td>
                    <td><%= campground.ipaddress %></td>
                    <td><%= campground.time %></td>
                </tr>
            <% }); %>
                </table>
        <% } %>
